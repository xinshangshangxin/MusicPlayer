{
    "esversion"     : 6,
    "maxerr"        : 200,
    "bitwise"       : true,
    "camelcase"     : false,
    "curly"         : true,
    "eqeqeq"        : true,
    "forin"         : false,
    "immed"         : false,
    "indent"        : 2,
    "latedef"       : true,
    "newcap"        : false,
    "noarg"         : true,
    "noempty"       : false,
    "nonew"         : true,
    "plusplus"      : false,
    "quotmark"      : "single",
    "undef"         : true,
    "unused"        : true,
    "strict"        : "global",
    "trailing"      : true,
    "maxparams"     : false,
    "maxdepth"      : false,
    "maxstatements" : false,
    "maxcomplexity" : 27,
    "maxlen"        : false,

    "evil"          : false,
    "expr"          : true,
    "funcscope"     : false,
    "iterator"      : false,
    "lastsemic"     : false,
    "laxbreak"      : false,
    "laxcomma"      : false,
    "loopfunc"      : false,
    "multistr"      : false,
    "proto"         : false,
    "scripturl"     : false,
    "smarttabs"     : false,
    "shadow"        : false,
    "sub"           : false,
    "supernew"      : false,
    "validthis"     : false,

    "devel"         : true,

    "node"          : true,
    "-W100"         : true,

    "latedef"       : "nofunc",

    "predef"        : [
                        "_",
                        "ApplicationError",
                        "config",
                        "logger",
                        "Promise"
                      ]
}
